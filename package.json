{
  "name": "starter-template--pnpm-parcel-typescript-react-eslint-prettier",
  "version": "0.0.1",
  "description": "Starter Template for frontend web development with Pnpm, Parcel, TypeScript, React, ESlint and Prettier.",
  "source": "src/index.html",
  "scripts": {
    "build": "parcel build",
    "serve": "parcel serve",
    "start": "parcel --open firefox --lazy",
    "watch": "parcel watch",
    "ts:lint": "tsc --noEmit -p tsconfig.check.json",
    "lint": "eslint ./src",
    "fix": "eslint --fix ./src",
    "prettier:check": "prettier --check src/**.{json,js,jsx,ts,tsx}",
    "prettier:fix": "prettier --write src/**.{json,js,jsx,ts,tsx}",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "unitecoder",
  "license": "MIT",
  "devDependencies": {
    "@parcel/config-default": "^2.6.0",
    "@parcel/core": "^2.6.0",
    "@parcel/transformer-sass": "^2.6.0",
    "@parcel/transformer-typescript-tsc": "^2.6.0",
    "@parcel/validator-typescript": "^2.6.0",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "parcel": "^2.6.0",
    "prettier": "^2.6.2",
    "process": "^0.11.10",
    "typescript": "^4.7.2"
  },
  "peerDependencies": {
    "@parcel/core": "^2.6.0"
  },
  "dependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  }
}
